{
  "engines": {
    "node": "20.11.*",
    "npm": "10.2.*"
  },
  "name": "menju",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next lint && next dev",
    "dev:prod": "dotenv -e .env.production -- npm run dev",
    "build": "next lint && next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "dotenv -e .env.development -- prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:seed": "dotenv -e .env.development -- prisma db push --force-reset && prisma db seed"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
