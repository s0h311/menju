    "eslint-plugin-unused-imports": "^3.0.0",
    "pino-pretty": "^10.2.3",
    "prettier": "3.0.3",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "dotenv -e .env.development -- ts-node --esm prisma/seed.ts"
  }
