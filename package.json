    "npm": "9.7.*",
    "yarn": "3.6.*"
  },
  "name": "menju",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next lint && next dev",
    "dev:prod": "dotenv -e .env.production -- yarn dev",
    "build": "next lint && next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "dotenv -e .env.development -- prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:seed": "dotenv -e .env.development -- prisma db push --force-reset && prisma db seed"
  },
