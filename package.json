    "pino-pretty": "^10.2.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "dotenv -e .env.development -- ts-node --esm prisma/seed.ts"
  }
}
